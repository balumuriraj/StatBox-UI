{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "forin": false,
    "quotemark": [true, "single"],
    "indent": [true, "spaces", 2],
    "interface-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": false,
    "no-shadowed-variable": false,
    "trailing-comma": [
      true,
      {
          "multiline": "never",
          "singleline": "never"
      }
    ]
  }
}
